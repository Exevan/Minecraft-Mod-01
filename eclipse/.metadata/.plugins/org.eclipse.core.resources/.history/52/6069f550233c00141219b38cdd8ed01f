package com.exevan.mod_01.init;

import com.exevan.mod_01.block.BlockMod_01;
import com.exevan.mod_01.block.BlockTest;
import com.exevan.mod_01.reference.Reference;

import cpw.mods.fml.common.registry.GameRegistry;
import cpw.mods.fml.common.registry.GameRegistry.ObjectHolder;

@ObjectHolder(Reference.MODID);
public class ModBlocks {
	
	public static final BlockMod_01 test = new BlockTest();
	
	public static void init() {
		GameRegistry.registerBlock(test, "blockTest");
	}

}
